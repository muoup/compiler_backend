define fn i32 main()
    %1 = call fib i32 10
    ret i32 %1
end

define fn i32 fib(i32 %n)
    %1 = icmp ule i32 %n, i32 1
    branch base_case recursive_case i1 %1
.base_case:
    ret i32 %n
.recursive_case:
    %2 = sub i32 %n, i32 1
    %3 = call fib i32 %2
    %4 = sub i32 %2, i32 1
    %5 = call fib i32 %4
    %6 = add i32 %5, i32 %3
    ret i32 %6
end