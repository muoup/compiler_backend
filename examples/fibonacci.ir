define fn i32 main()
    %1 = call fib 10
    ret %1
end

define fn i32 fib(i32 %n)
    %3 = icmp ule %n, 1
    branch base_case recursive_case %3
.base_case:
    ret %n
.recursive_case:
    %6 = sub %n, 1
    %7 = call fib %6
    %9 = sub %n, 2
    %10 = call fib %9
    %11 = add %10, %7
    ret %11

end