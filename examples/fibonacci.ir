define fn i32 fib(i32 %n)
    %ptr 1 = allocate 4
    store 4 - %n, %ptr 1
    %2 = load 4 - %ptr 1
    %3 = icmp ule - %2, 1
    if %3 then base_case else recursive_case;

base_case:
    %4 = load 4 - %ptr 1
    return - %4

recursive_case:
    %5 = load 4 - %ptr 1
    %6 = sub - %5, 1
    %7 = call fib - %6
    %8 = load 4 - %ptr 1
    %9 = sub - %8, 2
    %10 = call fib - %9
    %11 = add - %10, %7
    return - %5
end