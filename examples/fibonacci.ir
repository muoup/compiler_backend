define fn i32 fib(i32 %n)
    %ptr 1 = allocate 4
    store 4 %n, %ptr 1
    %2 = load 4 %ptr 1
    %3 = icmp ule %2, 1
    branch base_case recursive_case %3
.base_case:
    %4 = load 4 %ptr 1
    ret %4
.recursive_case:
    %5 = load 4 %ptr 1
    %6 = isub %5, 1
    %7 = call fib %6
    %8 = load 4 %ptr 1
    %9 = isub %8, 2
    %10 = call fib %9
    %11 = iadd %10, %7
    ret %11
end