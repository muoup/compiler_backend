define fn i32 main()
    %1 = call fib 10
    ret %1
end

define fn i32 fib(i32 %n)
    %1 = icmp ule %n, 1
    branch base_case recursive_case %1
.base_case:
    ret %n
.recursive_case:
    %2 = sub %n, 1
    %3 = call fib %2
    %4 = sub %n, 2
    %5 = call fib %4
    %6 = add %5, %3
    ret %6
end